{
    "name": "complete-statement",
    "displayName": "Complete Statement",
    "description": "complete statement for languages with a syntax similar to C",
    "version": "0.0.0",
    "publisher": "Jakukyo Friel",
    "engines": {
        "vscode": "^1.4.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.complete-statement"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.complete-statement",
                "title": "Complete Statement"
            }
        ],
        "keybindings": [
            {
                "key": "ctrl+;",
                "mac": "cmd+;",
                "when": "editorTextFocus",
                "command": "extension.complete-statement"
            }
        ]
    },
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    },
    "license": "0BSD",
    "homepage": "https://weakish.github.io/vscode-complete-statement",
    "repository": {
        "type": "git",
        "url": "https://weakish.github.io/vscode-complete-statement.git"
    },
    "bugs": {
        "url": "https://weakish.github.io/vscode-complete-statement/issues"
    },
    "keywords": [
        "completion",
        "semicolon",
        "c-syntax",
        "intelliJ"
    ],
    "preview": true
}
